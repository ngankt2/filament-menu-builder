{
    "name": "filament/filament-menu-builder",
    "description": "Create and manage menus and menu items",
    "keywords": [
        "laravel",
        "filament-menu-builder"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wiz\\FilamentMenuBuilder\\": "src/",
            "Wiz\\FilamentMenuBuilder\\Database\\Factories\\": "database/factories/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wiz\\FilamentMenuBuilder\\FilamentMenuBuilderServiceProvider"
            ],
            "aliases": {
                "FilamentMenuBuilder": "Wiz\\FilamentMenuBuilder\\Facades\\FilamentMenuBuilder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
